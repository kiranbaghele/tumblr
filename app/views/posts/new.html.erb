<h1>create new post</h1>
<div= "page_wrapper">
  <%= form_for :post, url: posts_path do |f|%>
    <div>
      <%= f.label :title%><br />
      <%= f.text_field :title%>
    </div>
    <div>
      <%= f.label :body%><br />
      <%= f.text_area :body%>
    </div><br />
    <%= f.submit %>
  <%end%>
</div>
